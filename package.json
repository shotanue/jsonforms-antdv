{
  "name": "@shotanue/jsonforms-antdv",
  "version": "0.0.1",
  "description": "JSON Forms renderer for Ant Design Vue",
  "homepage": "https://github.com/shotanue/jsonforms-antdv",
  "bugs": "https://github.com/shotanue/jsonforms-antdv/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shotanue/jsonforms-antdv"
  },
  "author": {
    "name": "shotanue",
    "email": "shotanue@gmail.com"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/jsonforms-antdv.umd.js",
  "module": "./dist/jsonforms-antdv.es.js",
  "exports": {
    ".": {
      "import": "./dist/jsonforms-antdv.es.js",
      "require": "./dist/jsonforms-antdv.umd.js"
    }
  },
  "scripts": {
    "dev": "vite dev --root=dev --config=vite.config.ts",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint src/**",
    "lint:fix": "eslint --fix src/**",
    "format": "prettier -c src",
    "format:fix": "prettier -w src",
    "qa": "run-s format lint",
    "qa:fix": "run-s format:fix lint:fix",
    "test": "cypress run-ct",
    "cypress-ct": "cypress open-ct",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gen:controlRenderer": "hygen generator createControlRenderer"
  },
  "dependencies": {
    "just-clone": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "0.10.0",
    "@babel/core": "7.16.0",
    "@cypress/vite-dev-server": "2.2.0",
    "@cypress/vue": "3.0.5",
    "@jsonforms/core": "3.0.0-alpha.2",
    "@jsonforms/vue": "3.0.0-alpha.2",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/cli": "6.3.12",
    "@storybook/testing-vue3": "0.0.1",
    "@storybook/vue3": "6.3.12",
    "@testing-library/cypress": "8.0.2",
    "@types/node": "16.11.9",
    "@vitejs/plugin-vue": "1.9.4",
    "@vue/compiler-sfc": "3.2.22",
    "ant-design-vue": "2.2.8",
    "babel-loader": "8.2.3",
    "cypress": "9.0.0",
    "esbuild-darwin-64": "0.13.14",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "hygen": "6.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "storybook-builder-vite": "0.1.8",
    "typescript": "4.5.2",
    "vite": "2.6.14",
    "vite-plugin-components": "0.13.3",
    "vite-plugin-dts": "0.9.4",
    "vue": "3.2.22",
    "vue-global-api": "0.4.1",
    "vue-loader": "16.8.3",
    "vue-template-compiler": "2.6.14",
    "vue-tsc": "0.29.5"
  },
  "peerDependencies": {
    "@jsonforms/core": "^3.0.0-alpha.1",
    "@jsonforms/vue": "^3.0.0-alpha.1",
    "ant-design-vue": "^2.2.6",
    "vue": "^3.0.0"
  }
}
