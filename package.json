{
  "name": "@shotanue/jsonforms-antdv",
  "version": "0.0.1",
  "description": "JSON Forms renderer for Ant Design Vue",
  "homepage": "https://github.com/shotanue/jsonforms-antdv",
  "bugs": "https://github.com/shotanue/jsonforms-antdv/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shotanue/jsonforms-antdv"
  },
  "author": {
    "name": "shotanue",
    "email": "shotanue@gmail.com"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/jsonforms-antdv.umd.js",
  "module": "./dist/jsonforms-antdv.es.js",
  "exports": {
    ".": {
      "import": "./dist/jsonforms-antdv.es.js",
      "require": "./dist/jsonforms-antdv.umd.js"
    }
  },
  "scripts": {
    "dev": "vite dev --root=dev --config=vite.config.ts",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint --fix src/**",
    "lint:check": "eslint src/**",
    "format": "prettier -w src",
    "format:check": "prettier -c src",
    "qa": "run-s format:check lint:check",
    "fix": "run-s format:fix lint:fix",
    "cypress-ct": "cypress open-ct",
    "cypress-ct:ci": "cypress run-ct",
    "clear": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.7.0",
    "@babel/core": "^7.15.0",
    "@cypress/vite-dev-server": "^2.0.7",
    "@cypress/vue": "^3.0.3",
    "@jsonforms/core": "^2.5.2",
    "@jsonforms/vue": "^2.5.2",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/cli": "^6.3.7",
    "@storybook/vue3": "^6.3.7",
    "@testing-library/cypress": "^8.0.0",
    "@types/node": "^16.7.1",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vue/compiler-sfc": "^3.2.4",
    "ant-design-vue": "^2.2.6",
    "babel-loader": "^8.2.2",
    "cypress": "^8.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "storybook-builder-vite": "^0.0.12",
    "typescript": "^4.3.5",
    "vite": "^2.5.0",
    "vite-plugin-components": "^0.13.2",
    "vite-plugin-dts": "^0.6.0",
    "vue": "^3.2.4",
    "vue-global-api": "^0.4.1",
    "vue-loader": "^16.5.0",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.2.3"
  },
  "peerDependencies": {
    "ant-design-vue": "^2.2.6",
    "vue": "^3.0.0"
  }
}
